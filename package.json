{
  "name": "@kaokei/use-react-service",
  "version": "1.0.3",
  "type": "module",
  "description": "Lightweight Vue 3 state management with dependency injection, inspired by Angular services.",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "unpkg": "./dist/index.cjs",
  "jsdelivr": "./dist/index.cjs",
  "files": [
    "dist/**/*"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./dist/*": "./dist/*",
    "./package.json": "./package.json"
  },
  "sideEffects": false,
  "homepage": "https://github.com/kaokei/use-react-service",
  "author": "kaokei",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kaokei/use-react-service/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kaokei/use-react-service.git"
  },
  "keywords": [
    "angular",
    "react",
    "service",
    "Ioc",
    "DI",
    "Dependency injection"
  ],
  "scripts": {
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "dev": "pnpm -F demo dev",
    "build": "vite build",
    "preview": "vite preview",
    "clean": "rm -rf dist/*",
    "public": "npm publish --access public",
    "postpublish": "git push origin --follow-tags",
    "prepublishOnly": "npm run clean && npm run build",
    "release": "standard-version -r",
    "release:first": "standard-version --first-release"
  },
  "devDependencies": {
    "@kaokei/di": "^3.0.9",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^22.15.17",
    "@types/prop-types": "^15.7.14",
    "@types/react": "^18.3.21",
    "@types/react-dom": "^18.3.7",
    "@vitejs/plugin-react": "^4.4.1",
    "@vue/reactivity": "^3.5.13",
    "cross-env": "^7.0.3",
    "globals": "^16.1.0",
    "jsdom": "^25.0.1",
    "prettier": "^3.5.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "rimraf": "^5.0.10",
    "standard-version": "^9.5.0",
    "tsup": "^8.4.0",
    "typescript": "^5.8.3",
    "vite": "^6.3.5",
    "vite-plugin-dts": "^4.5.3",
    "vitest": "^3.1.3"
  },
  "packageManager": "pnpm@9.9.0+sha512.60c18acd138bff695d339be6ad13f7e936eea6745660d4cc4a776d5247c540d0edee1a563695c183a66eb917ef88f2b4feb1fc25f32a7adcadc7aaf3438e99c1"
}
